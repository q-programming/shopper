{
  "compileOnSave": false,
  "compilerOptions": {
    "importHelpers": true,
    "paths": {
      "@services/*": [
        "app/services/*"
      ],
      "@model/*": [
        "app/model/*"
      ],
      "@env/*": [
        "environments/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "module": "es2015",
    "baseUrl": "./"
  }
}